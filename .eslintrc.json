{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "angular": true,
        "google": true
    },
    "rules": {
        "no-console": "warn",
        "eqeqeq": 1,
        "quotes": ["error", "single"],
        "semi": "error"
    }
}